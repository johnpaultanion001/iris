commands:
    01_enable_epel:
        command: "sudo amazon-linux-extras install epel -y"
    02_install_gcc_make:
        command: "sudo yum install -y gcc-c++ make"
    03_add_nodejs_yum_repository:
        command: "curl -sL https://rpm.nodesource.com/setup_16.x | sudo -E bash -"
    04_install_nodejs:
        command: "sudo yum install -y nodejs"
